###########################################################
# %HOME%\.ssh\config
###########################################################
StrictHostKeyChecking no
# Ignore "WARNING: REMOTE HOST IDENTIFICATION HAS CHANGED!"
UserKnownHostsFile=/dev/null
ServerAliveInterval 60


##
Host test1
    HostName 192.168.100.100
    User user1
    Port 22
    IdentityFile ~/.ssh/test.privatekey

